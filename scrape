#!/bin/bash
# Ultra simple scraper - now using AgentQL (formerly TinyFish)

# Check if API key exists
if [ -z "$AGENTQL_API_KEY" ]; then
    if [ -f ~/.openclaw/workspace/.env.agentql ]; then
        export $(cat ~/.openclaw/workspace/.env.agentql | grep -v '^#' | xargs)
    fi
fi

cd ~/.openclaw/workspace && python3 agentql-scraper.py "$@"